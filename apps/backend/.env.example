# Environment
NODE_ENV=development

# Database
DATABASE_URL="postgresql://flora_user:flora_password@localhost:5432/flora_db"
# DATABASE_URL="postgresql://USER_NAME:PASSWORD@localhost:5432/DB_NAME"

# Auth0
AUTH0_DOMAIN=your-auth0-domain
AUTH0_CLIENT_ID=your-client-id
AUTH0_CLIENT_SECRET=your-client-secret

# Stripe
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# Email Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=your_email@gmail.com
SMTP_PASS=your_app_password
FROM_EMAIL=noreply@flora.com
CONTACT_EMAIL=support@flora.com

# Frontend URL
FRONTEND_URL=http://localhost:5173

# JWT Secret (for additional auth if needed)
JWT_SECRET=your_jwt_secret_key_here

# Google Gemini AI
GEMINI_API_KEY=your_gemini_api_key_here

# Cloudinary Image Storage
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Google Distance Matrix API
GOOGLE_MAPS_API_KEY=your_google_maps_api_key

# Sendle API (Sandbox Only)
SENDLE_API_ID=your_sendle_api_id
SENDLE_API_KEY=your_sendle_api_key
SENDLE_SANDBOX_MODE=true
SENDLE_PICKUP_SUBURB=Melbourne
SENDLE_PICKUP_POSTCODE=3000

# Delivery Settings
DELIVERY_ORIGIN_LAT=-37.8136
DELIVERY_ORIGIN_LNG=144.9631
MAX_DELIVERY_DISTANCE_KM=50
# Enable Google Distance Matrix pricing (requires API key)
ENABLE_GOOGLE_DISTANCE=false
# Enable Sendle live quotes (requires Sendle credentials)
ENABLE_SENDLE_QUOTES=false
# Enable Sendle tracking API (requires Sendle credentials)
ENABLE_SENDLE_TRACKING=false

# Server
PORT=3001