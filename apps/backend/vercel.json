{
  "version": 2,
  "buildCommand": "cd ../.. && pnpm install && cd apps/backend && npx prisma generate && npx tsc",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.ts"
    }
  ],
  "functions": {
    "api/**/*.ts": {
      "maxDuration": 30,
      "memory": 1024
    }
  }
}
